@model EFitness.Models.Member

<h2>Edit Member</h2>

<form asp-action="Edit">
    <input type="hidden" asp-for="Id" />
    <div>
        <label>Full Name</label>
        <input asp-for="FullName" />
    </div>
    <div>
        <label>Email</label>
        <input asp-for="Email" />
    </div>
    <div>
        <label>Phone</label>
        <input asp-for="Phone" />
    </div>
    <div>
        <label>Goals</label>
        <input asp-for="Goals" />
    </div>
    <button type="submit">Save</button>
</form>
<a asp-action="Index">Back to List</a>
