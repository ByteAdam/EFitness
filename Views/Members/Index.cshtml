@model IEnumerable<EFitness.Models.Member>

<h2>Members</h2>
<table>
    <thead>
        <tr>
            <th>Full Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Goals</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var member in Model)
        {
            <tr>
                <td>@member.FullName</td>
                <td>@member.Email</td>
                <td>@member.Phone</td>
                <td>@member.Goals</td>
                <td>
                    <a asp-controller="Members" asp-action="Details" asp-route-id="@member.Id">Details</a> |
                    <a asp-controller="Members" asp-action="Edit" asp-route-id="@member.Id">Edit</a> |
                    <a asp-controller="Members" asp-action="Delete" asp-route-id="@member.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<a asp-controller="Members" asp-action="Create">Add New Member</a>