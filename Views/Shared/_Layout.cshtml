<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EFitness</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/EFitness.styles.css" asp-append-version="true" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1CmrxMRARb6aLqgBO7uuV1QpNzK3m9wRA6U9xzCZ+2usK5iqOUjNv3DXLACNFgm9" crossorigin="anonymous">

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
    <a class="navbar-brand" href="/">E-Fitness</a>
    <div class="collapse navbar-collapse">
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link" href="/Reservations">Rezervacije</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Members">Člani</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Activity1">Aktivnosti</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Achievements">Dosežki</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Instructors">Inštruktorji</a>
            </li>
        </ul>
    </div>
</nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - EFitness - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76A5F08Kt3d1r3taC40pt3xcPel6pDmAhUmgvYlrx0U5Y+/RtPjDeQoRhsdQghP" crossorigin="anonymous"></script>

</body>
</html>
